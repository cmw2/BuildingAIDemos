# AI Foundry Configuration
# Copy this file to .env and fill in your actual values

# Azure OpenAI Configuration (for most samples)
AZURE_OPENAI_ENDPOINT=https://your-resource-name.openai.azure.com
AZURE_OPENAI_API_KEY=your_api_key_here
AZURE_OPENAI_DEPLOYMENT_NAME=gpt-4o
AZURE_OPENAI_API_VERSION=2025-01-01-preview

# AI Foundry SDK Configuration (for samples 7 & 8)
AI_FOUNDRY_ENDPOINT=https://your-foundry-endpoint.com
AI_FOUNDRY_API_KEY=your_foundry_api_key_here

# Connected Storage Configuration (for sample 9)
AI_FOUNDRY_STORAGE_CONNECTION_NAME=image-storage
AZURE_STORAGE_CONTAINER_NAME=processed-images

# Application Insights Configuration (optional - for enhanced telemetry)
APPLICATIONINSIGHTS_CONNECTION_STRING=your_app_insights_connection_string_here

# Model Deployment Names
OPENAI_MODEL_DEPLOYMENT=gpt-4o
